{
	"name": "listd-media",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"prepare": "husky install",
		"dev": "concurrently \"vite dev\"",
		"vite:dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"typecheck": "npm run check",
		"lint": "eslint --ext .js,.cjs,.ts,.svelte .",
		"format": "prettier --plugin-search-dir . --check .",
		"lint:fix": "eslint --ext .js,.cjs,.ts,.svelte . --fix",
		"format:fix": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.34.3",
		"@skeletonlabs/skeleton": "^1.6.2",
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.20.1",
		"@tailwindcss/forms": "^0.5.3",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"autoprefixer": "^10.4.14",
		"concurrently": "^8.1.0",
		"eslint": "^8.42.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-airbnb-typescript": "^17.0.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-import-resolver-typescript": "^3.5.5",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-import-no-duplicates-prefix-resolved-path": "^2.0.0",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"postcss": "^8.4.24",
		"postcss-load-config": "^4.0.1",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.3.0",
		"svelte": "^3.59.1",
		"svelte-check": "^3.4.3",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.2",
		"tslib": "^2.5.3",
		"tsx": "^3.12.7",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vitest": "^0.31.4"
	},
	"type": "module",
	"dependencies": {
		"@tabler/icons-svelte": "^2.21.0",
		"eslint-plugin-svelte": "^2.30.0"
	},
	"license": "MIT"
}
